{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/setupTests.ts",
      "src/users.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "max-line-length": [true, 80],
    "no-console": false,
    "jsx-no-lambda": false,
    "no-var-requires": false
  }
}
